<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="milai.meishipintu.com.faxianlite">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--主页-->
        <activity
            android:name=".view.activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--启动页-->
        <activity
            android:name=".view.activity.StartActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <!--商品详情页-->
        <activity
            android:name=".view.activity.DetailsActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".view.activity.RegisteredActivity" />
        <activity android:name=".view.activity.LoginOrLoginActivity" />
        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.PasswordLoginActivity" />
        <activity android:name=".view.activity.OrderActivity" />
        <activity android:name=".view.activity.ParticipationDetailsActivity" />
        <activity android:name=".view.activity.SetPersonalInformationActivity" />
        <activity android:name=".view.activity.PersonalInformationActivity" />
        <activity android:name=".view.activity.WantActivity" />
        <activity android:name=".view.activity.ParticipateListActivity"></activity>
    </application>

</manifest>