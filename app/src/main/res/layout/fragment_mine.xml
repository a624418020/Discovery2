<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#f5f5f5">
         <RelativeLayout
             android:id="@+id/rl_01"
             android:layout_width="match_parent"
             android:layout_height="204dp"
             android:background="@color/three_three_three">
             <RelativeLayout
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:layout_centerVertical="true">
             <milai.meishipintu.com.faxianlite.Tool.CircleImageView
                 android:id="@+id/circleimageview"
                 android:layout_width="75dp"
                 android:layout_height="75dp"
                 android:layout_centerHorizontal="true"
                 android:src="@drawable/ic_launcher"
                 app:border_color="#000000"
                 app:border_width="1dp" />
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_centerHorizontal="true"
                 android:layout_below="@id/circleimageview"
                 android:text="用户名"
                 android:textColor="@color/white"
                 android:textSize="16sp"
                 android:layout_marginTop="11dp"/>
             </RelativeLayout>

         </RelativeLayout>
        <View
            android:id="@+id/view01"
            android:layout_width="match_parent"
            android:layout_height="6dp"
            android:layout_below="@id/rl_01"
            android:background="#D8D8D8">
        </View>
        <LinearLayout
            android:id="@+id/ll01"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:background="@color/white"
            android:layout_below="@id/view01">
            <RelativeLayout
                android:id="@+id/bt_order"
                android:layout_width="0dp"
                android:layout_weight="30"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true">
                    <ImageView
                        android:id="@+id/iv_01"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_quanbudingdan"/>
                    <TextView
                        android:layout_below="@+id/iv_01"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        style="@style/minetext"
                        android:text="我的订单"/>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/bt_likes"
                android:layout_width="0dp"
                android:layout_weight="30"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true">
                    <ImageView
                        android:id="@+id/iv_02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_my_want"/>
                    <TextView
                        android:layout_below="@+id/iv_02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        style="@style/minetext"
                        android:text="我想要"/>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/bt_card"
                android:layout_width="0dp"
                android:layout_weight="30"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true">
                    <ImageView
                        android:id="@+id/iv_03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_card" />
                    <TextView
                        android:layout_below="@id/iv_03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        style="@style/minetext"
                        android:text="卡包"/>
                </RelativeLayout>
            </RelativeLayout>

        </LinearLayout>
        <RelativeLayout
            android:id="@+id/rl02"
            android:layout_below="@id/ll01"
            android:layout_width="match_parent"
            android:layout_height="39.5dp"
            android:background="#F5F5F5">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:textSize="10sp"
                android:textColor="#999999"
                android:layout_centerVertical="true"
                android:text="服务"/>
        </RelativeLayout>
        <GridView
            android:id="@+id/gv_lattice"
            android:layout_below="@id/rl02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:horizontalSpacing="1dp"
            android:verticalSpacing="1dp"
            android:numColumns="3">
        </GridView>
        <include
            android:id="@+id/title_bar"
            layout="@layout/titlebar_mine" />
    </RelativeLayout>


</FrameLayout>
